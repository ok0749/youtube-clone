extends layouts/main.pug
include mixins/videoBlock
block content
    .search__header 
        h3 Searching for:  #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Fround
        each item in videos
            +videoBlock({
                id: item.id,
                title: item.title,
                videoFile: item.fileURL,
                views: item.views
            })
